{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "import", "jsx-a11y", "react", "react-hooks"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "airbnb/hooks",
    "prettier"
  ],
  "rules": { "import/prefer-default-export": "off" , "no-console": 1},
  "env": {
    "browser": true,
    "es2021": true
  }
}
